<table class="table table-striped">
    <thead>
        <td>Username</td><td>Email</td><td>Brewery</td>
    </thead>
{% for user in users %}
    <tr>
        <td>{{ user.username }}</td>
        <td>{{ user.email }}</td>
        <td>{{ user.brewery.breweryName }}</td>
    </tr>
{% endfor %}
</table>